{
    "name": "iot-dashboard",
    "version": "1.0.0",
    "description": "A Dashboard to connect iot devices to the internet and control through the dashboard",
    "main": "index.js",
    "scripts": {
        "build": "browserify index.js -o dist/bundle.js",
        "watch": "watchify index.js -o dist/bundle.js -v"
    },
    "keywords": [
        "iot",
        "iot-dashboard",
        "mqtt"
    ],
    "author": "vishal pandey",
    "license": "ISC",
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    },
    "dependencies": {
        "browserify": "^16.2.2",
        "mqtt": "^1.7.4",
        "watchify": "^3.11.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^8.0.0"
    }
}
